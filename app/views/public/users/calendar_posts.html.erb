<h1><%= @date %> の投稿一覧</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>タスク名</th>
      <th>実装時間</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.task.name %></td>
        <td><%= post.time_taken %> 分</td>
      </tr>
    <% end %>
  </tbody>
</table>
